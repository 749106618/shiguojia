.aui-timeLine-content {
    margin: 0 60rpx;
    border-left: 4rpx solid #e4e5e9;
}
.aui-timeLine-content-item {
    padding: 16rpx 12rpx 16rpx 0;
    margin-left: 36rpx;
    position: relative;
    font-size:24rpx;
    line-height: normal;
}
.aui-timeLine-content .aui-timeLine-content-item:first-child {
    margin-top: 16rpx;
    padding-top: 0;
    /* color: #ff8785; */
}
.aui-timeLine-content .aui-timeLine-content-item:first-child:before {
    content: "";
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    z-index: 0;
    top: -3px;
    left: -52rpx;
    background-color: #ffeac9;
    border-radius: 99rpx;
}
.aui-timeLine-content-icon {
    width: 20rpx;
    height: 20rpx;
    border-radius: 99rpx;
    background-color: #e4e5e9;
    content: "";
    position: absolute;
    z-index: 1;
    left: -37rpx;
    display: block;
    top: 20rpx;
    -webkit-transform: translate(-50%);
    transform: translate(-50%);
}
.aui-timeLine-content-item:first-child>.aui-timeLine-content-icon {
    top: 3rpx;
}
.aui-timeLine-content-item:first-child>.aui-timeLine-content-icon {
    background-color: #ffad29;
    width: 14rpx;
    height: 14rpx;
}

